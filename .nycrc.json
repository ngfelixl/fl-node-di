{
  "check-coverage": true,
  "per-file": true,
  "lines": [80, 95],
  "statements": [80, 95],
  "functions": [80, 95],
  "branches": [80, 95],
  "include": [
    "src/**/*.ts"
  ],
  "reporter": [
    "lcov",
    "text-summary"
  ],
  "require": [
    "ts-node/register"
  ],
  "extension": [
    ".ts"
  ],
  "sourceMap": false,
  "instrument": false,
  "all": true
}